{
    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/lab2",
            "name": "Debug STM32 lab2 (STLink)",
            "request": "launch",
            "type": "cortex-debug",
            "showDevDebugOutput": "none",
            "servertype": "stlink",
            "runToEntryPoint": "main",
            "device": "STM32F303K8",
            "v1": false,
            "svdFile": "${workspaceFolder}/../sys/CMSIS/SVD/STM32F303x.svd"
        },
        {
            "cwd": "${workspaceFolder}",
            "executable": "./build/lab2",
            "name": "Debug STM32 lab2 (ST-util)",
            "request": "launch",
            "type": "cortex-debug",
            "showDevDebugOutput": "none",
            "servertype": "stutil",
            "runToEntryPoint": "main",
            "device": "STM32F303K8",
            "v1": false,
            "svdFile": "${workspaceFolder}/../sys/CMSIS/SVD/STM32F303x.svd"
        },
        {
            "name": "C/C++: arm-none-eabi-gcc build and debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: arm-none-eabi-gcc build active file",
            "miDebuggerPath": "gdb"
        }
    ]
}