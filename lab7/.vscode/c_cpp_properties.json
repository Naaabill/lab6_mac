{
    "configurations": [
        {
            "name": "STM32 ARM",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/../sys/CMSIS/Device/ST/STM32F3xx/Include",
                "${workspaceFolder}/../sys/CMSIS/Include",
                "${workspaceFolder}/../lib/**"
            ],
            "defines": [
                "STM32F303x8",
                "ARMCM4"
            ],
            "compilerPath": "/opt/gcc-arm/bin/arm-none-eabi-gcc",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "gcc-arm",
            "compilerArgs": [
                "-mthumb",
                "-mcpu=cortex-m4",
                "-mfloat-abi=soft"
            ]
            //"configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}